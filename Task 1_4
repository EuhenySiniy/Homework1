package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        fibonacci();
        System.out.println(factorial(6));
    }

    public static void fibonacci() {
        int a = 1;
        int b = 1;
        int n;
        int sum;
        Scanner in = new Scanner(System.in);
        System.out.print("Введите желаемое количество чисел Фибоначчи: ");
        n = in.nextInt();
        for (int i = 0; i < n; i++) {
            sum = a + b;
            a = b;
            b = sum;
            System.out.print(sum + " ");
        }

    }

    static int factorial(int n) {
        int res = 1;
        for (int i = 1; i <= n; i++) {
            res = res * i;
        }
        return res;
    }
}
